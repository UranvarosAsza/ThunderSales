  "rules": {
    "excludePatterns": [
      {
        "pattern": "_event$",
        "description": "Exclude all vehicles ending with _event"
      },
      {
        "pattern": "_tutorial$",
        "description": "Exclude tutorial vehicles"
      },
      {
        "pattern": "_test$",
        "description": "Exclude test vehicles"
      },
      {
        "pattern": "_dev$",
        "description": "Exclude dev vehicles"
      }
    ],
    "excludeKeywords": ["tutorial", "test", "dev", "placeholder"],
    "excludePrefixes": ["temp_", "debug_"],
